
public class BusInfo {

	public Integer trip_id;
	public String arrival_time;
	public String departure_time;
	public int stop_id;
	public int stop_sequence;
	public String stop_headsign;
	public int pickup_type;
	public int dropoff_type;
	public double shape_dist_travelled;
	
	public String stop_code;
	public String stop_name;
	public String stop_desc;
	public String stop_lat;
	public String stop_lon;
	public String zone_id;
	public String stop_url;
	public String location_type;
	public String parent_station;
	
	
	
	
	public String outputInfo()
	{
		String outputInfo;
		outputInfo = "Trip ID                  || " + trip_id +"\n";
		outputInfo = outputInfo + "Arrival Time             || " + arrival_time+ "\n";
		outputInfo = outputInfo +"Departure Time           || " + departure_time+ "\n";
		outputInfo = outputInfo +"Stop ID                  || " + stop_id+ "\n";
		outputInfo = outputInfo +"Stop Sequence            || " + stop_sequence+ "\n";
		outputInfo = outputInfo +"Stop Headsign            || " + stop_headsign+ "\n";
		outputInfo = outputInfo +"Pickup Type              || " + pickup_type+ "\n";
		outputInfo = outputInfo +"Dropoff Type             || " + dropoff_type+ "\n";
		outputInfo = outputInfo +"Shape Distance Travelled || " + shape_dist_travelled+ "\n";
		
		outputInfo = outputInfo +"Stop Code                || " + stop_code+ "\n";
		outputInfo = outputInfo +"Stop Name                || " + stop_name+ "\n";
		outputInfo = outputInfo +"Stop Desc                || " + stop_desc+ "\n";
		outputInfo = outputInfo +"Stop Latitude            || " + stop_lat+ "\n";
		outputInfo = outputInfo +"Stop Longitude           || " + stop_lon+ "\n";
		outputInfo = outputInfo +"Zone ID                  || " + zone_id+ "\n";
		outputInfo = outputInfo +"Stop URL                 || " + stop_url+ "\n";
		outputInfo = outputInfo +"Location Type            || " + location_type+ "\n";
		outputInfo = outputInfo +"Parent Station           || " + parent_station+ "\n"+ "\n";
		return outputInfo;
	}
	
	public Integer getTrip_ID()
	{
		return this.trip_id;
	}
}
